{{pattern name="spacer"}}
<div class="project__container">
    <div class="row">
        <div class="col-md-4 offset-md-4">
            {{pattern name="heading" data="heading-work-project-one"}}
        </div>
    </div>

    <div class="row">
        <div class="col-12 col-md-6 offset-md-3 ">
            {{pattern name="paragraph" data="paragraph-work-project-one"}}
        </div>
    </div>

    {{pattern name="spacer"}}
    <div class="tasks__container">
        {{pattern name="heading" data="heading-work-project-one-sub-heading"}}
        {{pattern name="spacer"}}
        {{pattern name="list-items" data="list-items-project-one"}}
    </div>
    {{pattern name="spacer"}}
    <a class="projectlink" href="https://semharsium.github.io/project-sheep/">project link</a>
    {{pattern name="spacer" modifier="medium"}}
    <div class="row">
        <div class="col-12 col-lg-6 offset-lg-3 ">
        
            <pre class="prettyprint">
                $('.container').slick({ 
                    slidesToshow:1,
                    slidesToScroll:1,
                    arrows: true,
                    dots: true,
                    autoplay: true,
                    autoplayspeed: 2000,
                });          
        </pre>
        </div>
    </div>
</div>
{{pattern name="spacer" modifier="medium"}}